<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxPanel" name="DebuggerSettingsCommonPanel">
		<id_arg>0</id_arg>
		<object class="wxBoxSizer" variable="mainSizer" member="no">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="flexSizer" member="no">
					<cols>1</cols>
					<object class="sizeritem">
						<object class="wxCheckBox" name="ID_AUTOBUILD" variable="m_autoBuild" member="yes">
							<label>Auto-build project if it is not up-to-date</label>
						</object>
						<flag>wxTOP|wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="ID_AUTOSWITCH" variable="m_autoSwitch" member="yes">
							<label>When stopping, auto-switch to the first frame with valid source info</label>
						</object>
						<flag>wxTOP|wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="ID_DEBUGGERS_LOG" variable="m_debuggersLog" member="yes">
							<label>Display Debugger&apos;s log</label>
						</object>
						<flag>wxTOP|wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="ID_JUMP_ON_DOUBLE_CLICK" variable="m_jumpOnDoubleClick" member="yes">
							<label>Jump on Double-click in Stack trace window</label>
						</object>
						<flag>wxTOP|wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="ID_REQUIRE_CTRL_FOR_TOOLTIPS" variable="m_requireCtrlForTooltips" member="yes">
							<label>Require Control key to show the &apos;Evaluate expression&apos; tooltips</label>
						</object>
						<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
