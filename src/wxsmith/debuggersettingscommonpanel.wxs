<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxPanel" name="DebuggerSettingsCommonPanel">
		<id_arg>0</id_arg>
		<object class="wxBoxSizer" variable="mainSizer" member="no">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<object class="wxFlexGridSizer" variable="flexSizer" member="no">
					<cols>1</cols>
					<object class="sizeritem">
						<object class="wxCheckBox" name="ID_AUTOBUILD" variable="m_autoBuild" member="yes">
							<label>Auto-build project if it is not up-to-date</label>
						</object>
						<flag>wxTOP|wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="ID_AUTOSWITCH" variable="m_autoSwitch" member="yes">
							<label>When stopping, auto-switch to the first frame with valid source info</label>
						</object>
						<flag>wxTOP|wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="ID_DEBUGGERS_LOG" variable="m_debuggersLog" member="yes">
							<label>Full (Debug) log</label>
						</object>
						<flag>wxTOP|wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="ID_JUMP_ON_DOUBLE_CLICK" variable="m_jumpOnDoubleClick" member="yes">
							<label>Jump on Double-click in Stack trace window</label>
						</object>
						<flag>wxTOP|wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxCheckBox" name="ID_REQUIRE_CTRL_FOR_TOOLTIPS" variable="m_requireCtrlForTooltips" member="yes">
							<label>Require Control key to show the &apos;Evaluate expression&apos; tooltips</label>
						</object>
						<flag>wxTOP|wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxStaticBoxSizer" variable="valueTooltipSizer" member="no">
							<label>Value Tooltip Font</label>
							<object class="sizeritem">
								<object class="wxStaticText" name="ID_VALUE_TOOLTIP_LABEL" variable="m_valueTooltipLabel" member="yes">
									<label>This is a sample text</label>
								</object>
								<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
								<option>1</option>
							</object>
							<object class="sizeritem">
								<object class="wxButton" name="ID_BUTTON_CHOOSE_FONT" variable="chooseFont" member="no">
									<label>Choose</label>
									<handler function="OnChooseFontClick" entry="EVT_BUTTON" />
								</object>
								<flag>wxLEFT|wxALIGN_LEFT|wxALIGN_BOTTOM</flag>
								<border>5</border>
							</object>
						</object>
						<flag>wxTOP|wxLEFT|wxRIGHT|wxEXPAND|wxALIGN_LEFT|wxALIGN_BOTTOM</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxBoxSizer" variable="BoxSizer1" member="no">
							<object class="sizeritem">
								<object class="wxStaticText" name="wxID_ANY" variable="labelPerspective" member="no">
									<label>Perspective:</label>
								</object>
								<flag>wxTOP|wxBOTTOM|wxLEFT|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
								<border>5</border>
							</object>
							<object class="sizeritem">
								<object class="wxChoice" name="ID_CHOICE_PERSPECTIVE" variable="m_perspective" member="yes">
									<content>
										<item>Only one perspective</item>
										<item>One perspective per Debugger</item>
										<item>One perspective per Debugger configuration</item>
									</content>
									<selection>2</selection>
								</object>
								<flag>wxTOP|wxBOTTOM|wxLEFT|wxEXPAND|wxALIGN_LEFT|wxALIGN_BOTTOM</flag>
								<border>5</border>
								<option>1</option>
							</object>
						</object>
						<flag>wxTOP|wxLEFT|wxRIGHT|wxEXPAND|wxALIGN_LEFT|wxALIGN_BOTTOM</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
				<option>1</option>
			</object>
		</object>
	</object>
</wxsmith>
