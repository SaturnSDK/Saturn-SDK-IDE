<resource>
    <object class="wxPanel" name="ScriptConsole">
        <style>wxTAB_TRAVERSAL</style>
        <object class="wxBoxSizer" variable="BoxSizer1" member="yes">
            <orient>wxVERTICAL</orient>
            <object class="sizeritem">
                <option>1</option>
                <flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP|wxEXPAND</flag>
                <object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="txtConsole" member="yes">
                    <value></value>
                    <style>wxTE_MULTILINE|wxTE_READONLY|wxHSCROLL</style>
                </object>
            </object>
            <object class="sizeritem">
                <flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP|wxEXPAND</flag>
                <object class="wxPanel" name="ID_PANEL1" variable="Panel1" member="yes">
                    <bg>wxSYS_COLOUR_BTNFACE</bg>
                    <object class="wxBoxSizer" variable="BoxSizer2" member="yes">
                        <orient>wxHORIZONTAL</orient>
                        <object class="sizeritem">
                            <border>4</border>
                            <flag>wxLEFT|wxRIGHT|wxALIGN_CENTER</flag>
                            <object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
                                <label>Command:</label>
                            </object>
                        </object>
                        <object class="sizeritem">
                            <option>1</option>
                            <flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP|wxEXPAND</flag>
                            <object class="wxTextCtrl" name="ID_TEXTCTRL2" variable="txtCommand" member="yes">
                                <value></value>
                                <handler entry="EVT_TEXT_ENTER" function="OnbtnExecuteClick" />
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
                            <object class="wxButton" name="ID_BUTTON1" variable="btnExecute" member="yes">
                                <label>Execute</label>
                                <handler entry="EVT_BUTTON" function="OnbtnExecuteClick" />
                            </object>
                        </object>
                        <object class="sizeritem">
                            <border>16</border>
                            <flag>wxLEFT|wxALIGN_LEFT|wxALIGN_TOP</flag>
                            <object class="wxButton" name="ID_BUTTON2" variable="btnLoad" member="yes">
                                <label>Load from file</label>
                                <handler entry="EVT_BUTTON" function="OnbtnLoadClick" />
                            </object>
                        </object>
                        <object class="sizeritem">
                            <flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
                            <object class="wxButton" name="ID_BUTTON3" variable="btnClear" member="yes">
                                <label>Clear</label>
                                <handler entry="EVT_BUTTON" function="OnbtnClearClick" />
                            </object>
                        </object>
                    </object>
                </object>
            </object>
        </object>
    </object>
</resource>
