<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="5" />
	<Project>
		<Option title="EnvVars Plug-In" />
		<Option makefile="Makefile.linux" />
		<Option pch_mode="0" />
		<Option default_target="" />
		<Option compiler="gcc" />
		<Option virtualFolders="" />
		<Build>
			<Target title="default">
				<Option output="../../../devel/share/codeblocks/plugins/libenvvars.so" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="../../../devel/codeblocks" />
			</Target>
			<Environment>
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_CFG" value="" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="`wx-config --cflags`" />
			<Add option="-Winvalid-pch" />
			<Add option="-fexceptions" />
			<Add option="-fPIC" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DcbDEBUG" />
			<Add directory="$(#CB)/sdk" />
			<Add directory="$(#CB.INCLUDE)" />
			<Add directory="$(#CB)/sdk/wxscintilla/include" />
			<Add directory="$(#CB.INCLUDE)/wxscintilla/include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="`wx-config --libs`" />
			<Add library="codeblocks" />
			<Add directory="$(#CB)/devel" />
			<Add directory="$(#CB.LIB)" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ../../../devel/share/codeblocks/envvars.zip manifest.xml *.xrc" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="envvars.cpp">
			<Option compilerVar="CPP" />
			<Option target="default" />
		</Unit>
		<Unit filename="envvars.h">
			<Option compilerVar="" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="default" />
		</Unit>
		<Unit filename="envvars.xrc">
			<Option compilerVar="" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="default" />
		</Unit>
		<Extensions />
	</Project>
</CodeBlocks_project_file>