        echo "  The wxWidgets build which will be used by $PACKAGE_NAME $PACKAGE_VERSION"
        echo "  has the following settings:"

        if test "x$WX_DEBUG" = "x1" ; then
            echo "  - DEBUG build"
        elif test "x$WX_DEBUG" = "x0" ; then
            echo "  - RELEASE build"
        else
            echo "WX_DEBUG is $WX_DEBUG"
        fi


        if test "x$WX_UNICODE" = "x1" ; then
            echo "  - UNICODE mode"
