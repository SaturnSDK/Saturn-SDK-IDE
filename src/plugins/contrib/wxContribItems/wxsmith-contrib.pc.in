prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@/wxContribItems
includedir=@includedir@/wxContribItems
datarootdir = @datarootdir@
sharedir=@datadir@

Name: wxSmithContribItems
Description: wxWidgets contrib items also used for the wxSmith
Version: @VERSION@
Libs: -L${libdir} -lwxflatnotebook -lwxchartctrl -lwxcustombutton -lwximagepanel
Cflags: -fPIC -I${includedir} \
		-I${includedir}/wxFlatNotebook/include \
		-I${includedir}/wxchart-1.0/include \
		-I${includedir}/wxthings/include \
		-I${includedir}/wxImagePanel/include
