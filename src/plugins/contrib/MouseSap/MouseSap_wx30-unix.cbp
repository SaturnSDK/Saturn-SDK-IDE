<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="MouseSap wx3.0.x (Unix)" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel30/share/codeblocks/plugins/MouseSap" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs30/plugins/contrib/MouseSap" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="../../../devel30/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-ansi" />
					<Add option="$(#CB_RELEASE_TYPE)" />
					<Add option="`$(WX_CONFIG) --cflags`" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-fPIC" />
					<Add option="`pkg-config --cflags gtk+-2.0`" />
					<Add option="-DcbDEBUG" />
					<Add option="-DCB_PRECOMP" />
					<Add directory="../../../include" />
					<Add directory="../../../sdk/wxscintilla/include" />
				</Compiler>
				<Linker>
					<Add option="`$(WX_CONFIG) --libs`" />
					<Add option="`pkg-config --libs gtk+-2.0`" />
					<Add option="-Wl,--no-undefined" />
					<Add library="codeblocks" />
					<Add directory="../../../devel30" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ../../../devel30/share/codeblocks/MouseSap.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="WX_CONFIG" value="wx-config --version=3.0" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Unit filename="MouseSap.cpp" />
		<Unit filename="MouseSap.h" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
