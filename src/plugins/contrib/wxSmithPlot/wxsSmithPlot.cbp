		<Unit filename="wxsMathPlot.h">
			<Option target="wxSmithPlot" />
		</Unit>
		<Unit filename="wxsSmithPlot.cpp">
			<Option target="wxSmithPlot" />
		</Unit>
		<Unit filename="wxsSmithPlot.h">
			<Option target="wxSmithPlot" />
