			<Option target="MathPlot" />
		</Unit>
		<Unit filename="wxsAxis.cpp">
			<Option target="wxSmithPlot" />
		</Unit>
		<Unit filename="wxsAxis.h">
			<Option target="wxSmithPlot" />
