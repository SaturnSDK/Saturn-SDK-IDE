			<handler function="OnbDeleteAllClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="yes">
			<label>Edit Items</label>
			<pos>264,8</pos>
			<size>136,14</size>
			<style>wxST_NO_AUTORESIZE|wxALIGN_CENTRE</style>
		</object>
		<object class="wxBitmapButton" name="ID_BITMAPBUTTON6" variable="bEditItem" member="yes">
			<bitmap stock_id="wxART_HELP_SETTINGS" />
			<default>1</default>
			<pos>256,200</pos>
			<handler function="OnbEditItemClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT3" variable="StaticText3" member="yes">
			<label>Add Item</label>
			<pos>288,36</pos>
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT4" variable="StaticText4" member="yes">
			<label>Move Item Up</label>
			<pos>288,68</pos>
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT5" variable="StaticText5" member="yes">
			<label>Move Item Down</label>
			<pos>288,100</pos>
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT6" variable="StaticText6" member="yes">
			<label>Delete Item</label>
			<pos>288,132</pos>
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT7" variable="StaticText7" member="yes">
			<label>Delete All Items</label>
			<pos>288,164</pos>
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT8" variable="StaticText8" member="yes">
			<label>Edit Item Text</label>
			<pos>288,204</pos>
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT9" variable="StaticText9" member="yes">
