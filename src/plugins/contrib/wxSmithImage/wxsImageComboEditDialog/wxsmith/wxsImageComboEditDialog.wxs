		</object>
		<object class="wxComboBox" name="ID_COMBOBOX1" subclass="wxBitmapComboBox" variable="cbImage" member="yes">
			<content>
				<item>one</item>
				<item>two</item>
				<item>three</item>
			</content>
			<pos>256,272</pos>
			<size>88,22</size>
			<handler function="OncbImageSelect" entry="EVT_COMBOBOX" />
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT10" variable="StaticText10" member="yes">
