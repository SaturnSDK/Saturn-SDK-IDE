<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxDialog" name="wxsImageComboEditDialog">
		<title>Image-Combo-Box Item Editor</title>
		<size>414,350</size>
		<font>
			<size>8</size>
			<style>normal</style>
			<weight>normal</weight>
			<underlined>0</underlined>
			<family>swiss</family>
			<face>Arial</face>
		</font>
		<minsize>-1,-1</minsize>
		<maxsize>-1,-1</maxsize>
		<pos_arg>1</pos_arg>
		<size_arg>1</size_arg>
		<style>wxCAPTION|wxDEFAULT_DIALOG_STYLE</style>
		<object class="wxButton" name="ID_BUTTON1" variable="bOK" member="yes">
			<label>OK</label>
			<default>1</default>
			<pos>64,320</pos>
			<handler function="OnbOKClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxButton" name="ID_BUTTON2" variable="bCancel" member="yes">
			<label>Cancel</label>
			<pos>272,320</pos>
			<handler function="OnbCancelClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxTreeCtrl" name="ID_TREECTRL1" variable="Tree1" member="yes">
			<treedata>
				<node>! Define node data for a wxTreeCtrl.</node>
				<node>! Note that all lines that start with a &quot;!&quot; are comment lines and ignored.</node>
				<node>! Also, all empty lines are ignored (a line of spaces is not an empty line).</node>
				<node>!</node>
				<node>! The node data is organized similar to a Tabbed Hierarchical Values (THV) file,</node>
				<node>! such that each line of text represents one label for a single tree node.</node>
				<node>! The number of leading TAB characters indicate the indentation level of the</node>
				<node>! tree node.</node>
				<node>!</node>
				<node>! Space (or blank) characters are not used the same as TAB characters; if your</node>
				<node>! editor substitutes spaces for TABs, use the &quot;TAB Char&quot; attribute to change</node>
				<node>! the character used to indicate the tree node level.</node>
				<node>!</node>
				<node>! Following is a sample tree data definition:</node>
				<node></node>
				<node>First level, no tabs</node>
				<node>&#x09;Second level, 1 tab</node>
				<node>&#x09;&#x09;Third level, 2 tabs</node>
				<node>First level again, no tabs</node>
				<node>&#x09;Second level again, 1 tab</node>
				<node>&#x09;Another second level, 1 tab</node>
				<node>&#x09;&#x09;Third level again, 2 tabs</node>
				<node></node>
			</treedata>
			<pos>0,32</pos>
			<size>240,272</size>
			<style>wxTR_EDIT_LABELS|wxTR_NO_BUTTONS|wxTR_NO_LINES|wxTR_HIDE_ROOT|wxRAISED_BORDER</style>
		</object>
		<object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="yes">
			<label>Combo-Box</label>
			<pos>8,8</pos>
			<size>224,14</size>
			<style>wxST_NO_AUTORESIZE|wxALIGN_CENTRE</style>
		</object>
		<object class="wxBitmapButton" name="ID_BITMAPBUTTON1" variable="bAddItem" member="yes">
			<bitmap stock_id="wxART_ADD_BOOKMARK" />
			<default>1</default>
			<pos>256,32</pos>
			<handler function="OnbAddItemClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxBitmapButton" name="ID_BITMAPBUTTON2" variable="bMoveUp" member="yes">
			<bitmap stock_id="wxART_GO_UP" />
			<default>1</default>
			<pos>256,64</pos>
			<handler function="OnbMoveUpClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxBitmapButton" name="ID_BITMAPBUTTON3" variable="bMoveDown" member="yes">
			<bitmap stock_id="wxART_GO_DOWN" />
			<default>1</default>
			<pos>256,96</pos>
			<handler function="OnbMoveDownClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxBitmapButton" name="ID_BITMAPBUTTON4" variable="bDeleteItem" member="yes">
