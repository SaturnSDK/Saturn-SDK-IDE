<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Tools Plus Plugin wx2.9.x (Unix)" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel29/share/codeblocks/plugins/ToolsPlus" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs29/plugins/contrib/ToolsPlus" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="../../../devel29/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="$(#CB_RELEASE_TYPE)" />
				</Compiler>
			</Target>
			<Environment>
				<Variable name="WX_CONFIG" value="wx-config --version=2.9" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-ansi" />
			<Add option="`$(WX_CONFIG) --cflags`" />
			<Add option="-Winvalid-pch" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-fPIC" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
			<Add directory="../../../include" />
			<Add directory="../../../sdk" />
			<Add directory="../../../sdk/wxscintilla/include" />
			<Add directory="../../../include/mozilla_chardet" />
			<Add directory="../../../include/mozilla_chardet/mfbt" />
			<Add directory="../../../include/mozilla_chardet/nsprpub/pr/include" />
			<Add directory="../../../include/mozilla_chardet/xpcom" />
			<Add directory="../../../include/mozilla_chardet/xpcom/base" />
			<Add directory="../../../include/mozilla_chardet/xpcom/glue" />
		</Compiler>
		<Linker>
			<Add option="`$(WX_CONFIG) --libs std,aui`" />
			<Add option="-Wl,--no-undefined" />
			<Add library="codeblocks" />
			<Add library="wxscintilla_cb" />
			<Add directory="../../../devel29" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ../../../devel29/share/codeblocks/ToolsPlus.zip manifest.xml" />
			<Add after="./update29" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="CmdConfigDialog.cpp" />
		<Unit filename="CmdConfigDialog.h" />
		<Unit filename="PipedProcessCtrl.cpp" />
		<Unit filename="PipedProcessCtrl.h" />
		<Unit filename="Resources/dialogs.fbp" />
		<Unit filename="ShellCtrlBase.cpp" />
		<Unit filename="ShellCtrlBase.h" />
		<Unit filename="ToolsPlus.cpp" />
		<Unit filename="ToolsPlus.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="se_globals.cpp" />
		<Unit filename="se_globals.h" />
		<Unit filename="shellproperties.cpp" />
		<Unit filename="shellproperties.h" />
		<Unit filename="update" />
		<Unit filename="update.bat" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
