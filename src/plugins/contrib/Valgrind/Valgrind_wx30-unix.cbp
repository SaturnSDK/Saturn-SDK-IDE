<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Valgrind wx3.0.x (Unix)" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel30/share/codeblocks/plugins/Valgrind" prefix_auto="1" extension_auto="1" />
				<Option object_output="../../../.objs30/plugins/contrib/Valgrind" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="../../../devel30/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Option projectCompilerOptionsRelation="0" />
			</Target>
			<Environment>
				<Variable name="WX_CONFIG" value="wx-config --version=3.0" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wextra" />
			<Add option="-Wall" />
			<Add option="-ansi" />
			<Add option="$(#CB_RELEASE_TYPE)" />
			<Add option="`$(WX_CONFIG) --cflags`" />
			<Add option="-Winvalid-pch" />
			<Add option="-fexceptions" />
			<Add option="-fPIC" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DcbDEBUG" />
			<Add directory="../../../include" />
			<Add directory="../../../sdk/wxscintilla/include" />
		</Compiler>
		<Linker>
			<Add option="`$(WX_CONFIG) --libs`" />
			<Add option="-Wl,--no-undefined" />
			<Add library="libcodeblocks.a" />
			<Add directory="../../../devel30" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ../../../devel30/share/codeblocks/Valgrind.zip resources/manifest.xml" />
			<Add after="./update30" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="Valgrind.cpp" />
		<Unit filename="Valgrind.h" />
		<Unit filename="ValgrindListLog.cpp" />
		<Unit filename="ValgrindListLog.h" />
		<Unit filename="valgrind_config.cpp" />
		<Unit filename="valgrind_config.h" />
		<Unit filename="wxsmith/valgrind_config_panel.wxs" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<wxsmith version="1">
				<resources>
					<wxPanel wxs="wxsmith/valgrind_config_panel.wxs" src="valgrind_config.cpp" hdr="valgrind_config.h" fwddecl="1" i18n="1" name="ValgrindConfigurationPanel" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
