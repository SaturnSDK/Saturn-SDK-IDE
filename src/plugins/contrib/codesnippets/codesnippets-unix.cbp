<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Code Snippets Plugin (unix)" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug - Unix">
				<Option output="../../../devel/share/codeblocks/plugins/libcodesnippets.so" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs/2.6/plugins/contrib/codesnippets" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release - Unix">
				<Option output="../../../devel/share/codeblocks/plugins/libcodesnippets.so" prefix_auto="0" extension_auto="0" />
				<Option object_output="../../../.objs/2.6/plugins/contrib/codesnippets" />
				<Option type="3" />
				<Option compiler="gcc" />
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="Release - Unix;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="`wx-config --cflags`" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-fPIC" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DcbDEBUG" />
			<Add directory="../../../sdk" />
			<Add directory="../../../sdk/wxscintilla/include" />
		</Compiler>
		<Linker>
			<Add option="`wx-config --libs`" />
			<Add directory="../../../devel" />
		</Linker>
		<ExtraCommands>
			<Add after="./update" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="codesnippets.cpp" />
		<Unit filename="codesnippets.h" />
		<Unit filename="codesnippetstreectrl.cpp" />
		<Unit filename="codesnippetstreectrl.h" />
		<Unit filename="codesnippetswindow.cpp" />
		<Unit filename="codesnippetswindow.h" />
		<Unit filename="editsnippetdlg.cpp" />
		<Unit filename="editsnippetdlg.h" />
		<Unit filename="snippetitemdata.cpp" />
		<Unit filename="snippetitemdata.h" />
		<Extensions>
			<code_completion />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
