<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="FileManager Plugin wx3.0.x (64 bit)" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="..\..\..\devel30_64\share\CodeBlocks\plugins\FileManager" prefix_auto="1" extension_auto="1" />
				<Option object_output="..\..\..\.objs30_64\plugins\contrib\FileManager" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="..\..\..\devel30_64\codeblocks.exe" />
				<Compiler>
					<Add option="$(#CB_RELEASE_TYPE)" />
				</Compiler>
				<Linker>
					<Add library="codeblocks" />
					<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
					<Add directory="..\..\..\devel30_64" />
					<Add directory="$(#WX30_64.lib)\gcc_dll$(WX_CFG)" />
				</Linker>
			</Target>
			<Environment>
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_VERSION" value="30" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-D_WIN64" />
			<Add option="-DcbDEBUG" />
			<Add option="-DNOPCH" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add directory="..\..\..\include" />
			<Add directory="..\..\..\sdk\wxscintilla\include" />
			<Add directory="..\..\..\include\mozilla_chardet" />
			<Add directory="..\..\..\include\mozilla_chardet\mfbt" />
			<Add directory="..\..\..\include\mozilla_chardet\nsprpub\pr\include" />
			<Add directory="..\..\..\include\mozilla_chardet\xpcom" />
			<Add directory="..\..\..\include\mozilla_chardet\xpcom\base" />
			<Add directory="..\..\..\include\mozilla_chardet\xpcom\glue" />
			<Add directory="$(#WX30_64.include)" />
			<Add directory="$(#WX30_64.lib)\gcc_dll$(WX_CFG)\msw$(WX_SUFFIX)" />
		</Compiler>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel30_64\share\codeblocks\FileManager.zip manifest.xml Resources\fileexplorersettings.xrc" />
		</ExtraCommands>
		<Unit filename="FileExplorer.cpp" />
		<Unit filename="FileExplorer.h" />
		<Unit filename="FileExplorerSettings.cpp" />
		<Unit filename="FileExplorerSettings.h" />
		<Unit filename="FileExplorerUpdater.cpp" />
		<Unit filename="FileExplorerUpdater.h" />
		<Unit filename="FileManager.cpp" />
		<Unit filename="FileManager.h" />
		<Unit filename="directorymonitor.cpp" />
		<Unit filename="directorymonitor.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="se_globals.cpp" />
		<Unit filename="se_globals.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
