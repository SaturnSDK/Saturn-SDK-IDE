msgstr ""

#: src/tools/hunspell.cxx:1507
#, c-format
msgid "  --check-url\tCheck URLs, e-mail addresses and directory paths\n"
msgstr ""

#: src/tools/hunspell.cxx:1508
#, c-format
msgid "  -d d[,d2,...]\tuse d (d2 etc.) dictionaries\n"
msgstr ""

#: src/tools/hunspell.cxx:1509
#, c-format
