<resource>
    <object class="wxDialog" name="wxsNewWindowDlg">
        <object class="wxBoxSizer">
            <orient>wxVERTICAL</orient>
            <object class="sizeritem">
                <border>5</border>
                <flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxEXPAND</flag>
                <object class="wxStaticBoxSizer">
                    <orient>wxVERTICAL</orient>
                    <label>Options</label>
                    <object class="sizeritem">
                        <border>5</border>
                        <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxEXPAND</flag>
                        <object class="wxFlexGridSizer">
                            <rows>0</rows>
                            <cols>2</cols>
                            <vgap>5</vgap>
                            <hgap>5</hgap>
                            <growablecols>1</growablecols>
                            <object class="sizeritem">
                                <border>5</border>
                                <flag>wxALIGN_CENTER</flag>
                                <object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="no">
                                    <label>Class Name:</label>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <border>5</border>
                                <flag>wxALIGN_LEFT|wxALIGN_TOP|wxEXPAND</flag>
                                <object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="Class" member="yes">
                                    <value></value>
                                    <handler entry="EVT_TEXT" function="OnClassChanged" />
                                    <size>80,-1</size>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <border>5</border>
                                <flag>wxALIGN_CENTER</flag>
                                <object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="no">
                                    <label>Header file:</label>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <border>5</border>
                                <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxEXPAND</flag>
                                <object class="wxTextCtrl" name="ID_TEXTCTRL2" variable="Header" member="yes">
                                    <value></value>
                                    <handler entry="EVT_TEXT" function="OnHeaderChanged" />
                                    <size>80,-1</size>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <border>5</border>
                                <flag>wxALIGN_CENTER</flag>
                                <object class="wxStaticText" name="ID_STATICTEXT3" variable="StaticText3" member="no">
                                    <label>Source file:</label>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <border>5</border>
                                <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxEXPAND</flag>
                                <object class="wxTextCtrl" name="ID_TEXTCTRL3" variable="Source" member="yes">
                                    <value></value>
                                    <handler entry="EVT_TEXT" function="OnSourceChanged" />
                                    <size>80,-1</size>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <border>5</border>
                                <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxEXPAND</flag>
                                <object class="wxCheckBox" name="ID_CHECKBOX1" variable="UseXrc" member="yes">
                                    <label>Xrc File:</label>
                                    <handler entry="EVT_CHECKBOX" function="OnUseXrcChange" />
                                </object>
                            </object>
                            <object class="sizeritem">
                                <border>5</border>
                                <flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL|wxEXPAND</flag>
                                <object class="wxTextCtrl" name="ID_TEXTCTRL4" variable="Xrc" member="yes">
                                    <value></value>
                                    <handler entry="EVT_TEXT" function="OnXrcChanged" />
                                    <size>80,-1</size>
                                </object>
                            </object>
                        </object>
                    </object>
                    <object class="sizeritem">
                        <option>1</option>
                        <border>5</border>
                        <flag>wxALL|wxALIGN_CENTER</flag>
                        <object class="wxStaticText" name="ID_STATICTEXT4" variable="StaticText4" member="no">
                            <label>Warning:&#x0A;When adding resource in Xrc mode,&#x0A;this resource must be manually loaded.&#x0A;For details see wxXmlResource::Load()&#x0A;and wxXmlResource::Get().</label>
                            <style>wxALIGN_CENTRE</style>
                        </object>
                    </object>
                </object>
            </object>
            <object class="sizeritem">
                <border>5</border>
                <flag>wxALL|wxALIGN_CENTER</flag>
                <object class="wxBoxSizer">
                    <orient>wxHORIZONTAL</orient>
                    <object class="sizeritem">
                        <border>5</border>
                        <flag>wxALL|wxALIGN_CENTER</flag>
                        <object class="wxButton" name="ID_BUTTON1" variable="Button1" member="no">
                            <label>Cancel</label>
                            <handler entry="EVT_BUTTON" function="OnCancel" />
                        </object>
                    </object>
                    <object class="sizeritem">
                        <border>5</border>
                        <flag>wxALL|wxALIGN_CENTER</flag>
                        <object class="wxButton" name="ID_BUTTON2" variable="Button2" member="no">
                            <label>Create</label>
                            <default>1</default>
                            <handler entry="EVT_BUTTON" function="OnCreate" />
                        </object>
                    </object>
                </object>
            </object>
        </object>
    </object>
</resource>
