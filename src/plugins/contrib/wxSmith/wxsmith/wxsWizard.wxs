                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                                        <object class="wxCheckBox" name="ID_CHECKBOX2" variable="AddStatus" member="yes">
                                            <label>Add status bar</label>
                                            <checked>1</checked>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                                        <object class="wxCheckBox" name="ID_CHECKBOX3" variable="AddAbout" member="yes">
                                            <label>Create About dialog</label>
                                            <hiddedn>1</hiddedn>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                                        <object class="wxCheckBox" name="ID_CHECKBOX4" variable="UseXrc" member="yes">
                                            <label>Use XRC files</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                                        <object class="wxCheckBox" name="ID_CHECKBOX5" variable="AddDesc" member="yes">
                                            <label>Insert additional comments describing wxSmith&apos;s code</label>
                                        </object>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                    <object class="spacer">
                        <size>335,0</size>
                        <option>1</option>
                        <flag>wxALIGN_CENTER</flag>
                    </object>
                    <object class="sizeritem">
                        <option>1</option>
                        <border>5</border>
                        <flag>wxLEFT|wxRIGHT|wxBOTTOM|wxALIGN_CENTER|wxEXPAND</flag>
                        <object class="wxStaticBoxSizer" variable="wxWidgetsConfig" member="yes">
                            <orient>wxVERTICAL</orient>
                            <label>wxWigets library options</label>
                            <object class="sizeritem">
                                <option>1</option>
                                <flag>wxALIGN_CENTER|wxEXPAND</flag>
                                <object class="wxFlexGridSizer" variable="FlexGridSizer4" member="no">
                                    <rows>0</rows>
                                    <cols>2</cols>
                                    <vgap>0</vgap>
                                    <hgap>0</hgap>
                                    <growablecols>1</growablecols>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                                        <object class="wxStaticText" name="ID_STATICTEXT5" variable="StaticText5" member="no">
                                            <label>Configuration mode:</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxALIGN_CENTER|wxEXPAND</flag>
                                        <object class="wxComboBox" name="ID_COMBOBOX2" variable="ConfMode" member="yes">
                                            <selection>0</selection>
                                            <content>
                                                <item>Global variables</item>
                                                <item>Custom variables</item>
                                                <item>Library finder</item>
                                            </content>
                                            <handler entry="EVT_COMBOBOX" function="OnConfModeSelect" />
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxTOP|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                                        <object class="wxStaticText" name="ID_STATICTEXT4" variable="StaticText4" member="no">
                                            <label>Library type:</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxTOP|wxALIGN_CENTER|wxEXPAND</flag>
                                        <object class="wxComboBox" name="ID_COMBOBOX1" variable="LibType" member="yes">
                                            <selection>0</selection>
                                            <content>
                                                <item>DLL</item>
                                                <item>Static</item>
                                            </content>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxTOP|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                                        <object class="wxStaticText" name="ID_STATICTEXT6" variable="StaticText6" member="no">
                                            <label>wxWidgest dir:</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxALIGN_CENTER|wxEXPAND</flag>
                                        <object class="wxBoxSizer" variable="BoxSizer4" member="no">
                                            <orient>wxHORIZONTAL</orient>
                                            <object class="sizeritem">
                                                <option>1</option>
                                                <border>5</border>
                                                <flag>wxTOP|wxALIGN_CENTER|wxEXPAND</flag>
                                                <object class="wxTextCtrl" name="ID_TEXTCTRL4" variable="WxDir" member="yes">
                                                    <value></value>
                                                    <enabled>0</enabled>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <border>5</border>
                                                <flag>wxTOP|wxALIGN_CENTER</flag>
                                                <object class="wxButton" name="ID_BUTTON4" variable="WxDirChoose" member="yes">
                                                    <label>...</label>
                                                    <handler entry="EVT_BUTTON" function="OnwxDirChooseClick" />
                                                    <style>wxBU_EXACTFIT</style>
                                                    <enabled>0</enabled>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxTOP|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                                        <object class="wxStaticText" name="ID_STATICTEXT7" variable="StaticText7" member="no">
                                            <label>Configuration:</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxALL|wxALIGN_CENTER|wxEXPAND</flag>
                                        <object class="wxTextCtrl" name="ID_TEXTCTRL5" variable="WxConf" member="yes">
                                            <value></value>
                                            <enabled>0</enabled>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                                        <object class="wxStaticText" name="ID_STATICTEXT3" variable="UseUnicodeLabel" member="yes">
                                            <label>Use unicode:</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>4</border>
                                        <flag>wxLEFT|wxRIGHT|wxBOTTOM|wxALIGN_CENTER|wxEXPAND</flag>
                                        <object class="wxCheckBox" name="ID_CHECKBOX7" variable="UseUnicode" member="yes">
                                            <label></label>
                                        </object>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
            </object>
            <object class="sizeritem">
                <option>1</option>
                <border>8</border>
                <flag>wxLEFT|wxRIGHT|wxBOTTOM|wxALIGN_CENTER</flag>
                <object class="wxStdDialogButtonSizer" variable="StdDialogButtonSizer1" member="yes">
                    <object class="button">
                        <object class="wxButton" name="wxID_OK" />
                    </object>
                    <object class="button">
                        <object class="wxButton" name="wxID_CANCEL" />
                    </object>
                </object>
            </object>
        </object>
    </object>
</resource>
