<resource>
    <object class="wxDialog" name="wxsWizard">
        <title>wxSmith project wizzard</title>
        <centered>1</centered>
        <style>wxDEFAULT_DIALOG_STYLE</style>
        <object class="wxFlexGridSizer" variable="MainSizer" member="yes">
            <rows>0</rows>
            <cols>1</cols>
            <vgap>0</vgap>
            <hgap>0</hgap>
            <object class="sizeritem">
                <option>1</option>
                <border>4</border>
                <flag>wxBOTTOM|wxALIGN_CENTER|wxEXPAND</flag>
                <object class="wxFlexGridSizer" variable="FlexGridSizer2" member="no">
                    <rows>0</rows>
                    <cols>1</cols>
                    <vgap>0</vgap>
                    <hgap>0</hgap>
                    <growablecols>0</growablecols>
                    <growablerows>0,1</growablerows>
                    <object class="sizeritem">
                        <option>1</option>
                        <border>5</border>
                        <flag>wxALL|wxALIGN_CENTER|wxEXPAND</flag>
                        <object class="wxStaticBoxSizer" variable="StaticBoxSizer1" member="no">
                            <orient>wxVERTICAL</orient>
                            <label>wxSmith project options</label>
                            <object class="sizeritem">
                                <option>1</option>
                                <border>5</border>
                                <flag>wxALIGN_CENTER|wxEXPAND</flag>
                                <object class="wxFlexGridSizer" variable="FlexGridSizer3" member="no">
                                    <rows>0</rows>
                                    <cols>2</cols>
                                    <vgap>0</vgap>
                                    <hgap>0</hgap>
                                    <growablecols>1</growablecols>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxBOTTOM|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                                        <object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="no">
                                            <label>Project name:</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxBOTTOM|wxALIGN_CENTER|wxEXPAND</flag>
                                        <object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="PrjName" member="yes">
                                            <value>wxSmith project</value>
                                            <handler entry="EVT_TEXT" function="OnPrjNameText" />
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxBOTTOM|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                                        <object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="no">
                                            <label>Main frame title:</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxBOTTOM|wxALIGN_CENTER|wxEXPAND</flag>
                                        <object class="wxTextCtrl" name="ID_TEXTCTRL2" variable="FrmTitle" member="yes">
                                            <value>Welcome to wxSmith application</value>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxBOTTOM|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                                        <object class="wxStaticText" name="ID_STATICTEXT8" variable="StaticText8" member="yes">
                                            <label>Base directory:</label>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <flag>wxALIGN_CENTER|wxEXPAND</flag>
                                        <object class="wxBoxSizer" variable="BoxSizer5" member="yes">
                                            <orient>wxHORIZONTAL</orient>
                                            <object class="sizeritem">
                                                <option>1</option>
                                                <border>5</border>
                                                <flag>wxLEFT|wxBOTTOM|wxALIGN_CENTER</flag>
                                                <object class="wxTextCtrl" name="ID_TEXTCTRL6" variable="BaseDir" member="yes">
                                                    <value></value>
                                                    <handler entry="EVT_TEXT" function="OnBaseDirText" />
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <border>5</border>
                                                <flag>wxRIGHT|wxBOTTOM|wxALIGN_CENTER</flag>
                                                <object class="wxButton" name="ID_BUTTON5" variable="BaseDirChoose" member="yes">
                                                    <label>...</label>
                                                    <handler entry="EVT_BUTTON" function="OnBaseDirChooseClick" />
