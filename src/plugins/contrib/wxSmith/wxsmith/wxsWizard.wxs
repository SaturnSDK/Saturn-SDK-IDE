                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxALL|wxALIGN_CENTER</flag>
                                        <object class="wxCheckBox" name="ID_CHECKBOX6" variable="UseCustomPrjDir" member="yes">
                                            <label>Custom proj. dir:</label>
                                            <handler entry="EVT_CHECKBOX" function="OnUseCustomPrjDirChange" />
                                        </object>
                                    </object>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <flag>wxALIGN_CENTER|wxEXPAND</flag>
                                        <object class="wxBoxSizer" variable="BoxSizer2" member="no">
                                            <orient>wxHORIZONTAL</orient>
                                            <object class="sizeritem">
                                                <option>1</option>
                                                <border>5</border>
                                                <flag>wxLEFT|wxBOTTOM|wxALIGN_CENTER</flag>
                                                <object class="wxTextCtrl" name="ID_TEXTCTRL3" variable="PrjDir" member="yes">
                                                    <value></value>
                                                    <enabled>0</enabled>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <border>5</border>
                                                <flag>wxRIGHT|wxBOTTOM|wxALIGN_CENTER</flag>
                                                <object class="wxButton" name="ID_BUTTON3" variable="PrjDirChoose" member="yes">
                                                    <label>...</label>
                                                    <handler entry="EVT_BUTTON" function="OnDirChooseClick" />
                                                    <style>wxBU_EXACTFIT</style>
                                                    <enabled>0</enabled>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                            <object class="sizeritem">
                                <border>5</border>
                                <flag>wxTOP|wxBOTTOM|wxALIGN_CENTER|wxEXPAND</flag>
                                <object class="wxBoxSizer" variable="BoxSizer3" member="no">
                                    <orient>wxVERTICAL</orient>
                                    <object class="sizeritem">
                                        <option>1</option>
                                        <border>5</border>
                                        <flag>wxLEFT|wxRIGHT|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
                                        <object class="wxCheckBox" name="ID_CHECKBOX1" variable="AddMenu" member="yes">
