<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="5" />
	<Project>
		<Option title="copystrings" />
		<Option default_target="" />
		<Option compiler="gcc" />
		<Option virtualFolders="" />
		<Build>
			<Target title="default">
				<Option output="..\..\..\devel\share\CodeBlocks\plugins\copystrings.dll" />
				<Option type="3" />
				<Option compiler="gcc" />
			</Target>
			<Environment>
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_CFG" value="" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DWX_PRECOMP" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DcbDEBUG" />
			<Add option="-DTIXML_USE_STL" />
			<Add option="-DwxUSE_UNICODE" />
			<Add directory="$(#WX.include)" />
			<Add directory="$(#WX.lib)\gcc_dll$(WX_CFG)\msw$(WX_SUFFIX)" />
			<Add directory="$(#WX)\contrib\include" />
			<Add directory="$(#CB.include)" />
			<Add directory="$(#CB.include)\tinyxml" />
			<Add directory="$(#CB.include)\wxscintilla\include" />
			<Add directory="$(#CB)\sdk" />
			<Add directory="$(#CB)\sdk\tinyxml" />
			<Add directory="$(#CB)\sdk\wxscintilla\include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX.include)" />
		</ResourceCompiler>
		<Linker>
			<Add library="codeblocks" />
			<Add library="wxscintilla" />
			<Add library="wxmsw26$(WX_SUFFIX)" />
			<Add directory="$(#WX.lib)\gcc_dll$(WX_CFG)" />
			<Add directory="$(#CB.lib)" />
			<Add directory="$(#CB)\devel" />
			<Add directory="$(#CB)\sdk\tinyxml" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\devel\share\CodeBlocks\copystrings.zip manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="copystrings.cpp">
			<Option compilerVar="CPP" />
			<Option target="default" />
		</Unit>
		<Unit filename="copystrings.h">
			<Option compilerVar="CPP" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="default" />
		</Unit>
		<Unit filename="manifest.xml">
			<Option compilerVar="CPP" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="default" />
		</Unit>
		<Extensions />
	</Project>
</CodeBlocks_project_file>