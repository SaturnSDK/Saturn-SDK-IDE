<resource>
    <object class="wxDialog" name="RegExDlg">
        <title>Regular expressions testbed</title>
        <handler entry="EVT_INIT_DIALOG" function="OnInit" />
        <style>wxDEFAULT_DIALOG_STYLE</style>
        <object class="wxBoxSizer">
            <orient>wxVERTICAL</orient>
            <object class="sizeritem">
                <border>5</border>
                <flag>wxLEFT|wxRIGHT|wxTOP|wxALIGN_LEFT|wxALIGN_TOP</flag>
                <object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="no">
                    <label>Regular expression:</label>
                </object>
            </object>
            <object class="sizeritem">
                <border>5</border>
                <flag>wxLEFT|wxRIGHT|wxBOTTOM|wxALIGN_LEFT|wxALIGN_TOP|wxEXPAND</flag>
                <object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="txtRegEx" member="yes">
                    <value></value>
                    <size>410,24</size>
                </object>
            </object>
            <object class="sizeritem">
                <border>5</border>
                <flag>wxLEFT|wxRIGHT|wxTOP|wxALIGN_LEFT|wxALIGN_TOP</flag>
                <object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="no">
                    <label>Test input:</label>
                </object>
            </object>
            <object class="sizeritem">
                <border>5</border>
                <flag>wxLEFT|wxRIGHT|wxBOTTOM|wxALIGN_LEFT|wxALIGN_TOP|wxEXPAND</flag>
                <object class="wxTextCtrl" name="ID_TEXTCTRL2" variable="txtInput" member="yes">
                    <value></value>
                    <style>wxTE_MULTILINE</style>
                </object>
            </object>
            <object class="sizeritem">
                <border>5</border>
                <flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP|wxEXPAND</flag>
                <object class="wxStaticLine" name="ID_STATICLINE1" variable="StaticLine1" member="no">
                    <size>10,-1</size>
                </object>
            </object>
            <object class="sizeritem">
                <border>5</border>
                <flag>wxALL|wxALIGN_CENTER</flag>
                <object class="wxBoxSizer">
                    <orient>wxHORIZONTAL</orient>
                    <object class="sizeritem">
                        <flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP</flag>
                        <object class="wxButton" name="ID_BUTTON1" variable="btnTest" member="yes">
                            <label>Test</label>
                            <default>1</default>
                            <handler entry="EVT_BUTTON" function="OnTestClick" />
                        </object>
                    </object>
                    <object class="sizeritem">
                        <border>5</border>
                        <flag>wxLEFT|wxALIGN_LEFT|wxALIGN_TOP</flag>
                        <object class="wxButton" name="wxID_CANCEL" variable="btnCancel" member="yes">
                            <label>Close</label>
                        </object>
                    </object>
                </object>
            </object>
        </object>
    </object>
</resource>
