<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="5" />
	<Project>
		<Option title="RegExTestbed" />
		<Option pch_mode="2" />
		<Option default_target="" />
		<Option compiler="gcc" />
		<Option virtualFolders="" />
		<Build>
			<Target title="default">
				<Option output="RegExTestbed.dll" prefix_auto="1" extension_auto="1" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="..\..\..\devel\codeblocks.exe" />
				<Compiler>
					<Add option="-g" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DTIXML_USE_STL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWX_PRECOMP" />
					<Add option="-DwxUSE_UNICODE" />
					<Add directory="..\..\..\sdk" />
					<Add directory="..\..\..\sdk\wxscintilla\include" />
					<Add directory="$(#wx.include)" />
					<Add directory="$(#wx.lib)\gcc_dll\mswu" />
				</Compiler>
				<Linker>
					<Add library="codeblocks" />
					<Add library="wxmsw26u" />
					<Add directory="..\..\..\devel" />
					<Add directory="$(#wx.lib)\gcc_dll" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ..\..\..\devel\share\codeblocks\RegExTestbed.zip manifest.xml *.xrc" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Unit filename="RegExTestbed.cpp">
			<Option compilerVar="CPP" />
			<Option target="default" />
		</Unit>
		<Unit filename="RegExTestbed.h">
			<Option compilerVar="CPP" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="default" />
		</Unit>
		<Unit filename="manifest.xml">
			<Option compilerVar="CPP" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="default" />
		</Unit>
		<Unit filename="regexdlg.cpp">
			<Option compilerVar="CPP" />
			<Option target="default" />
		</Unit>
		<Unit filename="regexdlg.h">
			<Option compilerVar="CPP" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="default" />
		</Unit>
		<Extensions>
			<wxsmith>
				<dialog wxs_file="RegExDlg.wxs" class="RegExDlg" src_file="regexdlg.cpp" header_file="regexdlg.h" xrc_file="regexdlg.xrc" edit_mode="Xrc" />
				<configuration app_src_file="" main_resource="" init_all_handlers="necessary" />
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
