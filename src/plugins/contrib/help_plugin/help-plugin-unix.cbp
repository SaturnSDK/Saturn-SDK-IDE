<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Help Plugin (Unix)" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="help_plugin">
				<Option output="../../../devel/share/codeblocks/plugins/libhelp_plugin.so" prefix_auto="0" extension_auto="0" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="../../../devel/codeblocks.exe" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
				</Compiler>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="help_plugin;" />
		</VirtualTargets>
		<Compiler>
			<Add option="`wx-config --cflags`" />
			<Add option="-Winvalid-pch" />
			<Add option='-include &quot;sdk.h&quot;' />
			<Add option="-fexceptions" />
			<Add option="-fPIC" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DcbDEBUG" />
			<Add directory="../../../include" />
			<Add directory="../../../include/wxscintilla/include" />
		</Compiler>
		<Linker>
			<Add option="`wx-config --libs`" />
			<Add library="codeblocks" />
			<Add library="wxscintilla" />
			<Add directory="../../../devel" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ../../../devel/share/codeblocks/help_plugin.zip manifest.xml *.xrc" />
		</ExtraCommands>
		<Unit filename="HelpConfigDialog.cpp" />
		<Unit filename="HelpConfigDialog.h" />
		<Unit filename="help_common.cpp" />
		<Unit filename="help_common.h" />
		<Unit filename="help_plugin.cpp" />
		<Unit filename="help_plugin.h" />
		<Unit filename="help_plugin.xrc" />
		<Extensions>
			<code_completion />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
