<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="wxSmithGBSizer" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="wxSmithGBSizer">
				<Option output="..\..\..\devel\share\CodeBlocks\plugins\wxSmithGBSizer" prefix_auto="1" extension_auto="1" />
				<Option working_dir="..\..\..\devel" />
				<Option object_output="..\..\..\.objs\plugins\contrib\wxSmithGBSizer" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log -na -ns -nd" />
				<Option host_application=".\codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DcbDEBUG" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWXMAKINGDLL_PROPGRID" />
					<Add option="-DwxPG_USE_WXMODULE=0" />
					<Add directory="..\..\..\include" />
					<Add directory="..\..\..\sdk\wxscintilla\include" />
					<Add directory="..\..\..\sdk\wxpropgrid\include" />
					<Add directory="..\wxSmith" />
					<Add directory="..\wxSmith\wxwidgets\properties" />
				</Compiler>
				<Linker>
					<Add option="-mthreads" />
					<Add option="-Wl,--enable-auto-image-base" />
					<Add option="-Wl,--export-all-symbols" />
					<Add option="-Wl,--enable-auto-import" />
					<Add library="wxsmithlib" />
					<Add library="wxpropgrid" />
					<Add library="codeblocks" />
					<Add library="wxmsw28$(WX_SUFFIX)" />
					<Add directory="..\..\..\devel" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ..\..\..\devel\share\CodeBlocks\wxSmithGBSizer.zip manifest.xml" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="WX_CFG" value="" />
				<Variable name="WX_SUFFIX" value="u" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="wxSmithGBSizer;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DWX_PRECOMP" />
			<Add directory="$(#wx.lib)\gcc_dll$(WX_CFG)\msw$(WX_SUFFIX)" />
			<Add directory="$(#wx.include)" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#wx)\include" />
		</ResourceCompiler>
		<Linker>
			<Add directory="$(#wx.lib)\gcc_dll" />
		</Linker>
		<Unit filename="manifest.xml" />
		<Unit filename="wxSmithGBSizer.cpp" />
		<Unit filename="wxSmithGBSizer.h" />
		<Unit filename="wxsGridBagSizer.cpp" />
		<Unit filename="wxsGridBagSizer.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
