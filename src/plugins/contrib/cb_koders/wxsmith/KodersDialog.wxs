<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxScrollingDialog" name="KodersDialog">
		<title>OLOHO query</title>
		<centered>1</centered>
		<object class="wxBoxSizer" variable="bszMain" member="yes">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="bszIntro" member="yes">
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_LBL_INTRO" variable="lblIntro" member="yes">
							<label>Specify search to query the OLOHO webpage:</label>
						</object>
						<flag>wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
					</object>
				</object>
				<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
			</object>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="bszSearch" member="yes">
					<object class="sizeritem">
						<object class="wxTextCtrl" name="ID_TXT_SEARCH" variable="txtSearch" member="yes">
							<tooltip>Enter keyword to search for (at koders)</tooltip>
						</object>
						<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="ID_BTN_SEARCH" variable="btnSearch" member="yes">
							<label>Search</label>
							<default>1</default>
							<tooltip>Click to search at the koders webpage...</tooltip>
							<handler function="OnBtnSearchClick" entry="EVT_BUTTON" />
						</object>
						<flag>wxLEFT|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
					</object>
				</object>
				<flag>wxTOP|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
			</object>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="bszFilter" member="yes">
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_LBL_FILTER" variable="lblFilter" member="yes">
							<label>Filter:</label>
						</object>
						<flag>wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxChoice" name="ID_CHO_LANGUAGES" variable="choLanguages" member="yes">
							<content>
								<item>All Languages</item>
								<item>Ada</item>
								<item>ASP</item>
								<item>Assembler</item>
								<item>C</item>
								<item>C#</item>
								<item>Cpp</item>
								<item>ColdFusion</item>
								<item>Delphi</item>
								<item>Eiffel</item>
								<item>Erlang</item>
								<item>Fortran</item>
								<item>Java</item>
								<item>JavaScript</item>
								<item>JSP</item>
								<item>Lisp</item>
								<item>Lua</item>
								<item>Mathematica</item>
								<item>Matlab</item>
								<item>ObjectiveC</item>
								<item>Perl</item>
								<item>PHP</item>
								<item>Prolog</item>
								<item>Python</item>
								<item>Ruby</item>
								<item>Scheme</item>
								<item>Smalltalk</item>
								<item>SQL</item>
								<item>Tcl</item>
								<item>VB</item>
								<item>VB.NET</item>
							</content>
							<tooltip>Limit search to a specific programming language</tooltip>
						</object>
						<flag>wxLEFT|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxTOP|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
			</object>
		</object>
	</object>
</wxsmith>
