<resource>
    <object class="wxPanel" name="LanguagePanel">
        <style>wxTAB_TRAVERSAL</style>
        <object class="wxBoxSizer" variable="BoxSizer1" member="no">
            <orient>wxVERTICAL</orient>
            <object class="sizeritem">
                <option>1</option>
                <border>8</border>
                <flag>wxALL|wxALIGN_LEFT|wxALIGN_TOP|wxEXPAND</flag>
                <object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="no">
                    <label>The files for this project are available for the following&#x0A;languages. Please choose one from the list:</label>
                </object>
            </object>
            <object class="sizeritem">
                <border>8</border>
                <flag>wxALL|wxALIGN_CENTER</flag>
                <object class="wxStaticBoxSizer" variable="StaticBoxSizer1" member="no">
                    <orient>wxVERTICAL</orient>
                    <label>Language</label>
                    <object class="sizeritem">
                        <option>1</option>
                        <border>4</border>
                        <flag>wxALL|wxALIGN_CENTER|wxEXPAND</flag>
                        <object class="wxListBox" name="ID_LISTBOX1" variable="lstLanguages" member="yes" />
                    </object>
                </object>
            </object>
        </object>
    </object>
</resource>
