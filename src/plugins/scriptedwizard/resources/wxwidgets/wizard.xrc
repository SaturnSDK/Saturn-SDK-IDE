        </object>
      </object>
      <object class="sizeritem">
        <option>0</option>
        <flag>wxALL</flag>
        <border>8</border>
        <object class="wxCheckBox" name="chkWxQa">
          <label>wxWidgets QA Library</label>
          <checked>0</checked>
        </object>
      </object>
      <object class="sizeritem">
        <option>0</option>
        <flag>wxALL</flag>
        <border>8</border>
        <object class="wxCheckBox" name="chkWxRichText">
          <label>wxWidgets RichText Library</label>
          <checked>0</checked>
        </object>
      </object>
    </object>
  </object>
  <object class="wxPanel" name="WxProjDetails">
    <style>wxTAB_TRAVERSAL</style>
    <object class="wxBoxSizer">
      <orient>wxVERTICAL</orient>
      <object class="sizeritem">
        <option>0</option>
        <flag>wxALL</flag>
        <border>8</border>
        <object class="wxStaticText" name="m_staticText3">
          <label>Please Enter Project Details. Details will be used in labelling\nsource code of project.</label>
        </object>
      </object>
      <object class="sizeritem">
        <option>1</option>
        <flag>wxALL|wxEXPAND</flag>
        <border>8</border>
        <object class="wxFlexGridSizer">
          <rows>3</rows>
          <cols>2</cols>
          <vgap>8</vgap>
          <hgap>8</hgap>
          <growablecols>1</growablecols>
          <growablerows></growablerows>
          <object class="sizeritem">
            <option>0</option>
            <flag>wxALL</flag>
            <border>0</border>
            <object class="wxStaticText" name="m_staticText4">
              <label>Author:</label>
            </object>
          </object>
          <object class="sizeritem">
            <option>0</option>
            <flag>wxALL|wxEXPAND</flag>
            <border>0</border>
            <object class="wxTextCtrl" name="txtProjAuthor">
              <value></value>
              <maxlength>0</maxlength>
            </object>
          </object>
          <object class="sizeritem">
            <option>0</option>
            <flag>wxALL</flag>
            <border>0</border>
            <object class="wxStaticText" name="m_staticText5">
              <label>Author's email:</label>
            </object>
          </object>
          <object class="sizeritem">
            <option>0</option>
            <flag>wxALL|wxEXPAND</flag>
            <border>0</border>
            <object class="wxTextCtrl" name="txtProjEmail">
              <value></value>
              <maxlength>0</maxlength>
            </object>
          </object>
          <object class="sizeritem">
            <option>0</option>
            <flag>wxALL</flag>
            <border>0</border>
            <object class="wxStaticText" name="m_staticText6">
              <label>Author's website:</label>
            </object>
          </object>
          <object class="sizeritem">
            <option>0</option>
            <flag>wxALL|wxEXPAND</flag>
            <border>0</border>
            <object class="wxTextCtrl" name="txtProjWebsite">
              <value></value>
              <maxlength>0</maxlength>
            </object>
          </object>
        </object>
      </object>
    </object>
  </object>
</resource>