				<border>8</border>
				<object class="wxCheckBox" name="chkWxRegExp">
					<label>wxWidgets Regular Expression Library</label>
					<checked>0</checked>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL</flag>
				<border>8</border>
				<object class="wxCheckBox" name="chkWxExpat">
					<label>wxWidgets Expat Library</label>
					<checked>0</checked>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL</flag>
				<border>8</border>
				<object class="wxCheckBox" name="chkWxXml">
					<label>wxWidgets XML Library</label>
					<checked>0</checked>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL</flag>
				<border>8</border>
				<object class="wxCheckBox" name="chkWxXrc">
					<label>wxWidgets XRC Library</label>
					<checked>0</checked>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL|wxGROW</flag>
				<border>8</border>
				<object class="wxCheckBox" name="chkWxAdv">
					<label>wxWidgets Adv Library</label>
					<checked>0</checked>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL</flag>
				<border>8</border>
				<object class="wxCheckBox" name="chkWxMore">
					<font>
						<weight>bold</weight>
					</font>
					<label>Add more wxWidgets libraries</label>
					<checked>0</checked>
				</object>
			</object>
		</object>
	</object>
