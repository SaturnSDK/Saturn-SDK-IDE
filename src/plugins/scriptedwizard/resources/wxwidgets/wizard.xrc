<?xml version="1.0" encoding="utf-8"?>
<resource>
  <object class="wxPanel" name="WxConf">
		<style>wxTAB_TRAVERSAL</style>
		<object class="wxBoxSizer">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL|wxEXPAND</flag>
				<border>8</border>
				<object class="wxStaticText" name="m_staticText1">
					<label>Please select various configuration options.\n\nWhat you select here, must match the installed wxWidgets\nlibrary&apos;s settings.</label>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL|wxEXPAND</flag>
				<border>8</border>
				<object class="wxStaticBoxSizer">
					<orient>wxVERTICAL</orient>
					<label>wxWidgets Library Settings</label>
					<object class="sizeritem">
						<option>0</option>
						<flag>wxALL</flag>
						<border>8</border>
						<object class="wxCheckBox" name="chkWxConfDLL">
							<label>Use wxWidgets DLL</label>
							<checked>0</checked>
						</object>
					</object>
					<object class="sizeritem">
						<option>0</option>
						<flag>wxALL</flag>
						<border>8</border>
						<object class="wxCheckBox" name="chkWxConfMono">
							<label>wxWidgets is built as a monolithic library</label>
							<checked>0</checked>
						</object>
					</object>
					<object class="sizeritem">
						<option>0</option>
						<flag>wxALL</flag>
						<border>8</border>
						<object class="wxCheckBox" name="chkWxConfUni">
							<label>Enable unicode</label>
							<checked>0</checked>
						</object>
					</object>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL|wxEXPAND</flag>
				<border>8</border>
				<object class="wxStaticBoxSizer">
					<orient>wxVERTICAL</orient>
					<label>Miscellaneous Settings</label>
					<object class="sizeritem">
						<option>0</option>
						<flag>wxALL</flag>
						<border>8</border>
						<object class="wxCheckBox" name="chkWxEmpty">
							<label>Create Empty Project</label>
							<checked>0</checked>
						</object>
					</object>
					<object class="sizeritem">
						<option>0</option>
						<flag>wxALL|wxEXPAND</flag>
						<border>8</border>
						<object class="wxCheckBox" name="chkWxConfPCH">
							<label>Create and use precompiled header (PCH)</label>
							<checked>0</checked>
						</object>
					</object>
					<object class="sizeritem">
						<option>1</option>
						<flag>wxEXPAND</flag>
						<border>0</border>
						<object class="wxBoxSizer">
							<orient>wxHORIZONTAL</orient>
							<object class="sizeritem">
								<option>0</option>
								<flag>wxALL</flag>
								<border>8</border>
								<object class="wxStaticText" name="m_staticText2">
									<label>Configuration:</label>
								</object>
							</object>
							<object class="sizeritem">
								<option>0</option>
								<flag>wxALIGN_CENTER_VERTICAL|wxLEFT</flag>
								<border>8</border>
								<object class="wxTextCtrl" name="txtWxConfConfig">
									<value></value>
									<maxlength>0</maxlength>
								</object>
							</object>
						</object>
					</object>
					<object class="sizeritem">
						<option>0</option>
						<flag>wxALL</flag>
						<border>8</border>
						<object class="wxCheckBox" name="chkWxConfDebug">
							<label>Set Target Types (For Advanced users only)</label>
							<checked>0</checked>
						</object>
					</object>
				</object>
			</object>
		</object>
	</object>
  <object class="wxPanel" name="WxConfUnix">
		<style>wxTAB_TRAVERSAL</style>
		<object class="wxBoxSizer">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL|wxEXPAND</flag>
				<border>8</border>
				<object class="wxStaticText" name="m_staticText1">
					<label>Please select whether you want this project to use a\nprecompiled header (PCH).\n\nNote that if your compiler doesn&apos;t support them, this option\nwill be ignored...</label>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL</flag>
				<border>8</border>
				<object class="wxCheckBox" name="chkWxConfSo">
					<label>Use Dynamic Lib</label>
					<checked>0</checked>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL</flag>
				<border>8</border>
				<object class="wxCheckBox" name="chkWxConfUnicode">
					<label>Use Unicode Lib</label>
					<checked>0</checked>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL</flag>
				<border>8</border>
				<object class="wxCheckBox" name="chkWxUnixEmpty">
					<label>Create Empty Project</label>
					<checked>0</checked>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL|wxEXPAND</flag>
				<border>8</border>
				<object class="wxCheckBox" name="chkWxConfPCH">
					<label>Create and use precompiled header (PCH)</label>
					<checked>1</checked>
				</object>
			</object>
		</object>
	</object>
	<object class="wxPanel" name="WxConfTType">
		<style>wxTAB_TRAVERSAL</style>
		<object class="wxBoxSizer">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL</flag>
				<border>8</border>
				<object class="wxStaticText" name="m_staticText2">
					<label>Please select Executable Type for each target</label>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL</flag>
				<border>8</border>
				<object class="wxStaticText" name="m_staticText5">
					<font>
						<style>normal</style>
						<weight>bold</weight>
					</font>
					<label>Don't change them unless you need it.\nYou may need to change some settings manually.</label>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL|wxEXPAND</flag>
				<border>5</border>
				<object class="wxRadioBox" name="RadioBoxDebug">
					<style>wxRA_SPECIFY_COLS</style>
					<label>Debug Target</label>
					<content>
						<item>Console Mode Application</item>
						<item>GUI Mode Application</item>
					</content>
					<selection>1</selection>
					<dimension>1</dimension>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL|wxEXPAND</flag>
				<border>8</border>
				<object class="wxRadioBox" name="RadioBoxRelease">
					<style>wxRA_SPECIFY_COLS</style>
					<label>Release Target</label>
					<content>
						<item>Console Mode Application</item>
						<item>GUI Mode Application</item>
					</content>
					<selection>1</selection>
					<dimension>1</dimension>
				</object>
			</object>
		</object>
	</object>
  <object class="wxPanel" name="WxAddLib">
		<style>wxTAB_TRAVERSAL</style>
		<object class="wxBoxSizer">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL</flag>
				<border>8</border>
				<object class="wxStaticText" name="LabelHeading">
					<label>Please select additional libraries you want to add to project.\n\nPlease Note that Code::Blocks will NOT check for the\nexistence of any Libraries added below.</label>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL</flag>
				<border>8</border>
				<object class="wxCheckBox" name="chkWxJpeg">
					<label>wxWidgets JPEG Library</label>
					<checked>0</checked>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL</flag>
				<border>8</border>
				<object class="wxCheckBox" name="chkWxTiff">
					<label>wxWidgets TIFF Library</label>
					<checked>0</checked>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
