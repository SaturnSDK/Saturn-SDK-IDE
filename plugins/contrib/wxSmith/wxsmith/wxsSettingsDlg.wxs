<resource>
    <object class="wxDialog" name="wxsSettingsDlg">
        <title>wxSmith Configuration</title>
        <centered>1</centered>
        <object class="wxFlexGridSizer" variable="FlexGridSizer1" member="yes">
            <rows>0</rows>
            <cols>1</cols>
            <vgap>0</vgap>
            <hgap>0</hgap>
            <growablecols>0</growablecols>
            <growablerows>0</growablerows>
            <object class="sizeritem">
                <option>1</option>
                <border>5</border>
                <flag>wxALL|wxALIGN_CENTER</flag>
                <object class="wxNotebook" name="ID_NOTEBOOK1" variable="Notebook1" member="no">
                    <object class="notebookpage">
                        <label>Editor</label>
                        <selected>1</selected>
                        <object class="wxPanel" name="ID_PANEL1" variable="Panel1" member="no">
                            <object class="wxFlexGridSizer" variable="FlexGridSizer2" member="yes">
                                <rows>0</rows>
                                <cols>2</cols>
                                <vgap>0</vgap>
                                <hgap>0</hgap>
                                <growablecols>1</growablecols>
                                <object class="sizeritem">
                                    <option>1</option>
                                    <border>5</border>
                                    <flag>wxALL|wxALIGN_CENTER</flag>
                                    <object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="no">
                                        <label>Drag Assistance Type</label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <option>1</option>
                                    <border>5</border>
                                    <flag>wxALL|wxALIGN_CENTER|wxEXPAND</flag>
                                    <object class="wxComboBox" name="ID_COMBOBOX1" variable="DragAssistType" member="yes">
                                        <selection>2</selection>
                                        <content>
                                            <item>None</item>
                                            <item>Simple</item>
                                            <item>Colour Mix</item>
                                        </content>
                                        <handler entry="EVT_COMBOBOX" function="OnDragAssistTypeSelect" />
                                        <style>wxCB_READONLY</style>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <option>1</option>
                                    <border>5</border>
                                    <flag>wxALL|wxALIGN_CENTER</flag>
                                    <object class="wxStaticText" name="ID_STATICTEXT3" variable="StaticText3" member="no">
                                        <label>Drag target colour</label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <option>1</option>
                                    <border>5</border>
                                    <flag>wxALL|wxALIGN_CENTER|wxEXPAND</flag>
                                    <object class="wxButton" name="ID_BUTTON1" variable="DragTargetCol" member="yes">
                                        <label>...</label>
                                        <handler entry="EVT_BUTTON" function="OnDragTargetColClick" />
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <option>1</option>
                                    <border>5</border>
                                    <flag>wxALL|wxALIGN_CENTER</flag>
                                    <object class="wxStaticText" name="ID_STATICTEXT4" variable="StaticText4" member="no">
                                        <label>Drag Parent Colour</label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <option>1</option>
                                    <border>5</border>
                                    <flag>wxALL|wxALIGN_CENTER|wxEXPAND</flag>
                                    <object class="wxButton" name="ID_BUTTON2" variable="DragParentCol" member="yes">
                                        <label>...</label>
                                        <handler entry="EVT_BUTTON" function="OnDragParentColClick" />
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <option>1</option>
                                    <border>5</border>
                                    <flag>wxALL|wxALIGN_CENTER</flag>
                                    <object class="wxStaticText" name="ID_STATICTEXT5" variable="StaticText5" member="no">
                                        <label>Palette icon size</label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <option>1</option>
                                    <border>5</border>
                                    <flag>wxALIGN_CENTER</flag>
                                    <object class="wxFlexGridSizer" variable="FlexGridSizer3" member="yes">
                                        <rows>0</rows>
                                        <cols>0</cols>
                                        <vgap>0</vgap>
                                        <hgap>0</hgap>
                                        <object class="sizeritem">
                                            <option>1</option>
                                            <border>5</border>
                                            <flag>wxALL|wxALIGN_CENTER</flag>
                                            <object class="wxRadioButton" name="ID_RADIOBUTTON1" variable="Icons16" member="yes">
                                                <label>16x16</label>
                                                <style>wxRB_GROUP</style>
                                            </object>
                                        </object>
                                        <object class="sizeritem">
                                            <option>1</option>
                                            <border>5</border>
                                            <flag>wxALL|wxALIGN_CENTER</flag>
                                            <object class="wxRadioButton" name="ID_RADIOBUTTON2" variable="Icons32" member="yes">
                                                <label>32x32</label>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <option>1</option>
                                    <border>5</border>
                                    <flag>wxALL|wxALIGN_CENTER</flag>
                                    <object class="wxStaticText" name="ID_STATICTEXT6" variable="StaticText6" member="yes">
                                        <label>Tool icon size</label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <option>1</option>
                                    <border>5</border>
                                    <flag>wxALIGN_CENTER</flag>
                                    <object class="wxFlexGridSizer" variable="FlexGridSizer4" member="yes">
                                        <rows>0</rows>
                                        <cols>0</cols>
                                        <vgap>0</vgap>
                                        <hgap>0</hgap>
                                        <object class="sizeritem">
                                            <option>1</option>
                                            <border>5</border>
                                            <flag>wxALL|wxALIGN_CENTER</flag>
                                            <object class="wxRadioButton" name="ID_RADIOBUTTON3" variable="TIcons16" member="yes">
                                                <label>16x16</label>
                                                <style>wxRB_GROUP</style>
                                            </object>
                                        </object>
                                        <object class="sizeritem">
                                            <option>1</option>
                                            <border>5</border>
                                            <flag>wxALL|wxALIGN_CENTER</flag>
                                            <object class="wxRadioButton" name="ID_RADIOBUTTON4" variable="TIcons32" member="yes">
                                                <label>32x32</label>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                    <object class="notebookpage">
                        <label>Advanced</label>
                        <object class="wxPanel" name="ID_PANEL2" variable="Panel2" member="no">
                            <object class="wxFlexGridSizer" variable="FlexGridSizer5" member="yes">
                                <rows>0</rows>
                                <cols>0</cols>
                                <vgap>0</vgap>
                                <hgap>0</hgap>
                                <growablecols>1</growablecols>
                                <object class="sizeritem">
                                    <option>1</option>
                                    <border>5</border>
                                    <flag>wxALL|wxALIGN_CENTER</flag>
                                    <object class="wxStaticText" name="ID_STATICTEXT1" variable="StaticText1" member="no">
                                        <label>Preview fetch delay</label>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <option>1</option>
                                    <border>5</border>
                                    <flag>wxALL|wxALIGN_CENTER|wxEXPAND</flag>
                                    <object class="wxSpinCtrl" name="ID_SPINCTRL1" variable="PrevFetchDelay" member="yes">
                                        <value>50</value>
                                        <max>500</max>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
            </object>
            <object class="sizeritem">
                <option>1</option>
                <border>5</border>
                <flag>wxALL|wxALIGN_CENTER</flag>
                <object class="wxBoxSizer" variable="BoxSizer1" member="yes">
                    <orient>wxHORIZONTAL</orient>
                    <object class="sizeritem">
                        <option>1</option>
                        <border>5</border>
                        <flag>wxALL|wxALIGN_CENTER</flag>
                        <object class="wxButton" name="ID_BUTTON3" variable="BtnOk" member="no">
                            <label>Ok</label>
                            <handler entry="EVT_BUTTON" function="OnBtnOkClick" />
                        </object>
                    </object>
                    <object class="sizeritem">
                        <option>1</option>
                        <border>5</border>
                        <flag>wxALL|wxALIGN_CENTER</flag>
                        <object class="wxButton" name="ID_BUTTON4" variable="BtnCancel" member="yes">
                            <label>Cancel</label>
                            <handler entry="EVT_BUTTON" function="OnBtnCancelClick" />
                        </object>
                    </object>
                </object>
            </object>
        </object>
    </object>
</resource>
