<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="4" />
	<Project>
		<Option title="EnvVars Plug-In" />
		<Option makefile="Makefile.linux" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel/share/codeblocks/plugins/libenvvars.so" />
				<Option object_output="../../../.objs/2.6/plugins/contrib/envvars" />
				<Option deps_output="../../../.deps/2.6/plugins/contrib/envvars" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option host_application="../../../devel/codeblocks" />
				<Option includeInTargetAll="1" />
			</Target>
			<Environment>
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_CFG" value="Unicode" />
			</Environment>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="`wx-config --cflags`" />
			<Add option="-Winvalid-pch" />
			<Add option="-fexceptions" />
			<Add option="-fPIC" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DcbDEBUG" />
			<Add directory="../../../sdk" />
			<Add directory="../../../sdk/wxscintilla/include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="`wx-config --libs`" />
			<Add library="libcodeblocks.a" />
			<Add directory="../../../devel" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ../../../devel/share/codeblocks/envvars.zip *.xrc" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="envvars.cpp">
			<Option compilerVar="CPP" />
			<Option target="default" />
		</Unit>
		<Unit filename="envvars.h">
			<Option compilerVar="" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="default" />
		</Unit>
		<Unit filename="envvars.xrc">
			<Option compilerVar="" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="default" />
		</Unit>
	</Project>
</CodeBlocks_project_file>