<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="4" />
	<Project>
		<Option title="C::B Profiler (Unix)" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel/share/codeblocks/plugins/libcbprofiler.so" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option includeInTargetAll="1" />
			</Target>
			<Environment>
				<Variable name="CBDIR" value="../../.." />
			</Environment>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="`wx-config --cflags`" />
			<Add option="-fPIC" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add directory="$(CBDIR)/sdk" />
			<Add directory="$(CBDIR)/sdk/wxscintilla/include" />
		</Compiler>
		<Linker>
			<Add option="`wx-config --libs`" />
			<Add library="codeblocks" />
			<Add directory="$(CBDIR)/devel" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ../../../devel/share/codeblocks/profiler.zip resources/*.xrc" />
		</ExtraCommands>
		<Unit filename="cbprofiler.cpp">
			<Option compilerVar="CPP" />
			<Option target="default" />
		</Unit>
		<Unit filename="cbprofiler.h">
			<Option compilerVar="" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="default" />
		</Unit>
		<Unit filename="cbprofilerconfig.cpp">
			<Option compilerVar="CPP" />
			<Option target="default" />
		</Unit>
		<Unit filename="cbprofilerconfig.h">
			<Option compilerVar="" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="default" />
		</Unit>
		<Unit filename="cbprofilerexec.cpp">
			<Option compilerVar="CPP" />
			<Option target="default" />
		</Unit>
		<Unit filename="cbprofilerexec.h">
			<Option compilerVar="" />
			<Option compile="0" />
			<Option link="0" />
			<Option target="default" />
		</Unit>
	</Project>
</CodeBlocks_project_file>